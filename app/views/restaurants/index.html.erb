<h1>All Restaurants:</h1>
<p><%= link_to 'Add new restaurant', new_restaurant_path %></p>
<% @restaurants.each do |restaurant| %>
  <h2><%=restaurant.name%></h2>
  <p><%= link_to 'Show', restaurant_path(restaurant) %></p>
  <p><%= link_to 'Add review', new_restaurant_review_path(restaurant) %></p>
  <p><%= link_to 'Edit', edit_restaurant_path(restaurant) %></p>
<% end %>
